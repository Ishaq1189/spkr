POST        /authenticate        controllers.Application.authenticate()
POST        /user/add            controllers.Application.addUser()
GET         /user/search         controllers.Application.searchUser(q: String ?= "")
POST        /chat/add            controllers.Application.addChat()
GET         /chat/list           controllers.Application.listChats()
GET         /chat/history        controllers.Application.chatHistory(id: String ?= "")